    <children>
        <Pane prefHeight="800.0" prefWidth="600.0" style="-fx-background-color: black;">
            <children>
                <Button layoutX="152.0" layoutY="562.0" mnemonicParsing="false" onAction="#gameMenuS" prefHeight="50.0" prefWidth="300.0" style="-fx-background-color: yellow;" text="Confirm">
